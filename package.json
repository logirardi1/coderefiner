{
  "name": "coderefiner",
  "displayName": "CodeRefine",
  "description": "code formater for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },

  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.formatCode",
        "title": "Format Code"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.31",
    "eslint": "^7.21.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "typescript": "^4.2.3",
    "vscode-test": "^1.5.0"
  }
}